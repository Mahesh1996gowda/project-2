package Program;

import java.util.Scanner;

public class Anmstrongno {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a Number");
		int num=sc.nextInt();
		int copy=num;
		int count=0;
		int sum=0;
		while(num!=0) {
			int rem= num%10;
			count++;
			num=num/10;
			sum=rem*count;
		}System.out.println(sum);
	}

}
