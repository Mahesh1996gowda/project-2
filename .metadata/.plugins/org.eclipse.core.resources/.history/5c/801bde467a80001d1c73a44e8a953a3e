package Program;

import java.util.Scanner;

public class Numberssum {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number");
		{
		int num=sc.nextInt();
		int rem=num%10;
		int sum=0;
		sum=sum+rem;
		num= num/10;
	}System.out.println(sum);
	}
}
